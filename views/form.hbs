<div class="div bg-danger text-white mx-5">
      <h1 class="text-center">CUSTOM SUPPORT SERIVCES</h1>
    </div>
<div class="container">
    <form>
      <div class="form-group">
        <label for="tittle">Tên của bạn</label>
        <input type="text" class="form-control" id="tittle"  placeholder="Tiêu đề (required)" required>
        
      </div>
      <div class="form-group">
        <label for="Email">Email của bạn</label>
        <input type="email" class="form-control" id="Email" placeholder="Email (required)"required>
      </div>
      <div class="form-group">
        <label for="address">Địa chỉ của bạn</label>
        <input type="email" class="form-control" id="address" placeholder="Địa chỉ của bạn ">
      </div>
      <div class="form-group">
        <label for="phone">Điện thoại của bạn</label>
        <input type="email" class="form-control" id="phone" placeholder="Điện thoại của bạn">
      </div>
      <form>
        <div class="form-group">
          <label for="comment">Nội dung liên hệ</label>
          <textarea class="form-control" rows="5" id="comment"></textarea>
        </div>
      </form>
      <button type="submit" class="btn btn-primary">Gửi liên hệ</button>
    </form>
  </div>