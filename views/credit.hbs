<div class="menu-label mb-3 p-2 mx-5"><h1 class="ml-2">Staff info</h1></div>
<div id="credit" class="container-fluid card-group p-5">
    {{#each staff_infos}}
    <div class="col-lg-3 col-md-12 col-sm-12 mx-auto card u-shadow-v18 g-bg-white text-center rounded g-px-20 g-py-40 g-mb-5 h-100 card-profile">
        <div class="card-header d-flex align-items-center justify-content-center g-bg-white">
            <img src="images/logo & bg & etc/temp_avatar.png" alt="" class="img-thumbnail img-profile">
        </div>
        <div class="card-body d-flex flex-column">
            <h5 class="h5 g-color-black g-font-weight-600 g-mb-10 mt-0 mb-3">{{this.name}}</h5>
            <p>{{this.id}}</p>
            <p class="d-block g-color-primary g-font-size-16 mb-3 mt-auto quote">{{this.position}}</p>
            <p class="quote">{{this.quote}}</p>
        </div>
    </div>
    {{/each}}
</div>

